{
  "camelcase": false,
  "curly": true,
  "forin": false,
  "latedef": "nofunc",
  "newcap": false,
  "noarg": true,
  "nonew": true,
  "quotmark": "single",
  "undef": true,
  "unused": "vars",
  "trailing": true,
  "maxlen": 80,

  "eqnull": true,
  "esnext": true,
  "expr": true,

  "evil": true,

  "maxerr": 1000,
  "regexdash": true,
  "laxcomma": true,
  "proto": true,

  "browser": true,
  "devel": true,
  "nonstandard": true,
  "worker": true,
  "predef": [
    "setTimeout", "process", "setImmediate", "XMLHttpRequest", "prim", "parse"
  ]
}
